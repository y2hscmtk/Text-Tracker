{
    "manifest_version": 3,
    "name": "Text-Tracker",
    "version": "1.0",
    "description": "글자 수를 카운팅하는 프로그램입니다.",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"],
    "icons": {
        "16": "icons/text-tracker-16.png",
        "32": "icons/text-tracker-32.png",
        "48": "icons/text-tracker-48.png",
        "128": "icons/text-tracker-128.png"
    }
}
